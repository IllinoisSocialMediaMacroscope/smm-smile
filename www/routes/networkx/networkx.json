{
	"form-action": "networkx",
	"containers":[
		{"container-name":"file-container",
		"container-label-name":"Select from your folder",
		"inputs":[
					{"type":"select",
						"name":"selectFile",
						"id":"selectFile",
						"sytle":"",
						"class":"form-control",
						"optgroup": true
					}
				]},
		{"container-name":"network-relation-container",
		"container-label-name":"Network relation",
		"inputs":[
					{"type":"select",
						"name":"relations",
						"id":"relations",
						"class":"form-control",
						"optgroup": false,
						"options":[
							{"value":"Please Select...",
							"label":"Please Select..."},

							{"value":"retweet_from",
							"label":"retweet from"},
							
							{"value":"reply_to",
							"label":"reply to"},
							
							{"value":"mentions",
							"label":"mentions"}							
						]
					}
				]},

		{"container-name":"",
		"container-label-name":"Select graph layout",
		"inputs":[
					{"type":"select",
						"name":"layout",
						"id":"layout",
						"class":"form-control",
						"optgroup": false,
						"options":[
							{"value":"Please Select...",
								"label":"Please Select..."},

							{"value":"random",
							"label":"random layout"},
							
							{"value":"circular",
							"label":"circular layout"},
							
							{"value":"fruchterman",
							"label":" fruchterman layout"},
							
							{"value":"shell",
							"label":" shell layout"},
							
							{"value":"spectral",
							"label":" spectral layout"}
							
						]
					}
				]}		
	],
	"custom-script":"bootstrap/js/customized/view_networkx.js"
}
			