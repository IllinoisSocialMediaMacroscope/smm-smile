  "get": {
    "containers": [
      {
        "container-name": "",
        "container-label-name": "Preprocess step",
        "input": {
          "type": "select",
          "name": "process",
          "id": "process",
          "options": [
            {
              "value": "Please Select...",
              "label": "Please Select..."
            },
            {
              "value": "stemming",
              "label": "stemming"
            }
          ]
        }
      },
      {
        "container-name": "",
        "container-label-name": "Tagging",
        "input": {
          "type": "select",
          "name": "tagger",
          "id": "tagger",
          "options": [
            {
              "value": "Please Select...",
              "label": "Please Select..."
            },
            {
              "value": "posTag",
              "label": "Natural Language Tool Kit Part of Speech Tagger"
            }
          ]
        }
      }
    ],
    "buttonGroups": [
      {
        "id": "clear",
        "class": "btn btn-primary",
        "value": "Clear",
        "style": "margin: auto 5px;",
        "onclick": "customized_reset();"
      },
      {
        "id": "submit",
        "class": "btn btn-danger",
        "value": "Submit",
        "style": "margin: auto 5px;"
      }
    ],
    "citation": {
      "trigger_id": "tagger",
      "content": [
        {
          "text": ["<a href=\"https://scholar.google.com/scholar?hl=en&as_sdt=0%2C14&q=NLTK%3A+the+natural+language+toolkit.+In+Proceedings+of+the+COLING%2FACL+on+Interactive++++++++presentation+sessions+%28pp.+69-72%29.+Association+for+Computational+Linguistics&btnG=\" target=\"_blank\">Bird, S. (2006, July). NLTK: the natural language toolkit. In Proceedings of the COLING/ACL on Interactivepresentation sessions (pp. 69-72). Association for Computational Linguistics.</a>"],
          "condition": "posTag"
        }
      ]
    }
  },
  "post": {
    "cutoff":20000,
    "lambda_config": {
      "aws_lambda_function": "lambda_preprocessing"
