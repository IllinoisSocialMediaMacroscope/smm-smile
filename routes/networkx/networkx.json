{
	"form-action": "/networkx",
	"containers":[
		{"container-name":"file-container",
		"container-label-name":"select from your folder",
		"inputs":[
					{"type":"select",
						"name":"selectFile",
						"id":"selectFile",
						"sytle":"",
						"class":"form-control",
						"optgroup": true
					}
				]},
		{"container-name":"",
		"container-label-name":"network relation",
		"inputs":[
					{"type":"select",
						"name":"relationships",
						"id":"relationships",
						"class":"form-control",
						"optgroup": false,
						"options":[
							{"value":"reply_to",
							"label":"reply to"},
							
							{"value":"retweet_from",
							"label":"retweet from"},
							
							{"value":"mentions",
							"label":" user mentions"}							
						]
					}
				]},
		{"container-name":"",
		"container-label-name":"select graph layout",
		"inputs":[
					{"type":"select",
						"name":"layout",
						"id":"layout",
						"class":"form-control",
						"optgroup": false,
						"options":[
							{"value":"spring",
							"label":"spring layout"},
							
							{"value":"circular",
							"label":"circular layout"},
							
							{"value":"fruchterman",
							"label":" fruchterman layout"},
							
							{"value":"random",
							"label":" random layout"},
							
							{"value":"shell",
							"label":" shell layout"},
							
							{"value":"spectral",
							"label":" spectral layout"}
							
						]
					}
				]},
		{"container-name":"",
		"container-label-name":"node size",
		"inputs":[
					{"type":"range",
						"min":"5",
						"max":"100",
						"value":"10",
						"name":"node_size",
						"onchange":"rangeNode.value=value",
						"id":"node_size",
						"output_id":"rangeNode"
					}
				]},
		{"container-name":"",
		"container-label-name":"edge width",
		"inputs":[
					{"type":"range",
						"min":"1",
						"max":"20",
						"value":"10",
						"name":"edge_width",
						"onchange":"rangeEdge.value=value",
						"id":"edge_width",
						"output_id":"rangeEdge"
					}
				]}
	],
	"custom-script":"../bootstrap/js/customized/view_networkx.js"
}
			