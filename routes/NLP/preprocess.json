{
	"form-action": "/NLP/preprocess",
	"containers":[
		{"container-name":"",
		"container-label-name":"format",
		"inputs":[
					{"type":"radio",
						"name":"option",
						"id":"file",
						"value":"file",
						"sytle":"position: absolute; visibility: hidden;",
						"class":"customized-radio",
						"checked":true,
						"label":"file"
					},
					{"type":"radio",
						"name":"option",
						"id":"URL",
						"value":"URL",
						"sytle":"position: absolute; visibility: hidden;",
						"class":"customized-radio",
						"checked":false,
						"label":"URL"
					}
				]},
		{"container-name":"file-container",
		"container-label-name":"select from your folder",
		"inputs":[
					{"type":"select",
						"name":"selectFile",
						"id":"selectFile",
						"sytle":"",
						"class":"form-control",
						"optgroup": true
					}
				]},
		{"container-name":"url-container",
		"container-label-name":"URL",
		"inputs":[
					{"type":"text",
						"name":"input",
						"id":"link",
						"value":"https://techservices.illinois.edu/news/2015/telling-compelling-stories-social-media-analytics-team",
						"sytle":"",
						"class":"form-control col-md-7 col-xs-12"
					}
				]},
		{"container-name":"",
		"container-label-name":"preprocess step",
		"inputs":[
					{"type":"select",
						"name":"model",
						"id":"model",
						"class":"form-control",
						"optgroup": false,
						"options":[
							{"value":"lemmatization",
							"label":"lemmatization"},
							
							{"value":"stemming",
							"label":"stemming"},
							
							{"value":"both",
							"label":"lemmatization + stemming"}
						]
					}
				]},
		{"container-name":"",
		"container-label-name":"tagging",
		"inputs":[
					{"type":"select",
						"name":"tagger",
						"id":"tagger",
						"class":"form-control",
						"optgroup": false,
						"options":[{"value":"posTag",
									"label":"Natural Language Tool Kit Part of Speech Tagger"}]
					}
				]}
	],
	"custom-script":"../bootstrap/js/customized/view_preprocess.js"
}
			